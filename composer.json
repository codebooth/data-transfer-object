    "keywords": [
        "data-transfer-object",
        "data-structure",
        "codebooth"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
